<jsp:root xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:jsp="http://java.sun.com/JSP/Page"
          version="2.0">

    <jsp:directive.attribute name="title" required="true" />

    <div class="jive-contentBoxHeader">
        <c:out value="${title}" />
    </div>
    <div class="jive-contentBox">
        <jsp:doBody />
    </div>

</jsp:root>