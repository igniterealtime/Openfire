            <input type="checkbox" value="enabled" name="storeAvatarInDB" id="storeAvatarInDB" ${vcardBean.avatarStoredInDB ? "checked" : ""}/> <label for="storeAvatarInDB"><fmt:message key="setup.ldap.user.vcard.avatardb" /></label></p>

            <!-- BEGIN vcard table -->
